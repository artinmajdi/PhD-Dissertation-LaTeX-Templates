name: chest_xray

conda_env: conda.yaml

entry_points:
    main:
        parameters:
            epoch: {type: str, default: 3}
            bsize: {type: str, default: 30}
            max_sample: {type: str, default: 1000}
        command: python main.py --epoch {epoch} --bsize {bsize} --max_sample {max_sample}